    "window.title(\"Fractals\")\n",
    "window.configure(background=\"black\")\n",
    "Label (window, text= \"Choose Fractal Julia(J) or Mandlebrot(M):\", bg=\"black\", fg=\"white\", font=\"none 20 bold\") .grid(row=0, column=0, sticky=E)\n",
    "\n",
    "\n",
    "window.mainloop()\n"
